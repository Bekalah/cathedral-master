{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "pnpm run build:web",
        "outputDirectory": "apps/web/out"
      }
    },
    {
      "src": "apps/circuitum99/package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "pnpm run build:circuitum99",
        "outputDirectory": "apps/circuitum99/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/circuitum99/(.*)",
      "dest": "/apps/circuitum99/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/web/out/$1"
    }
  ],
  "functions": {
    "api/egregore/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "regions": ["iad1"],
  "env": {
    "NODE_ENV": "production"
  }
}